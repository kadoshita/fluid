{
  "name": "@fluid/backend",
  "version": "2.0.0-alpha.3",
  "description": "",
  "main": "build/main.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config -r ./src/opentelemetry.ts src/main.ts dotenv_config_path=.env.local",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:push:production": "NODE_ENV=production drizzle-kit push:pg",
    "postgres:start": "docker run --rm -d --name postgres -p 5432:5432 -v $(pwd)/postgres:/var/lib/postgres -e POSTGRES_PASSWORD=postgres postgres:15.5",
    "postgres:stop": "docker stop postgres"
  },
  "author": "sublimer <sublimer.me@gmail.com> (https://sublimer.me/)",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@google-cloud/opentelemetry-cloud-monitoring-exporter": "^0.18.0",
    "@google-cloud/opentelemetry-cloud-trace-exporter": "^2.2.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.46.0",
    "@opentelemetry/exporter-metrics-otlp-grpc": "^0.51.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.51.1",
    "@opentelemetry/sdk-metrics": "^1.24.1",
    "@opentelemetry/sdk-trace-base": "^1.24.1",
    "@supabase/supabase-js": "^2.43.1",
    "drizzle-orm": "^0.30.10",
    "fastify": "^4.27.0",
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "drizzle-kit": "^0.20.18",
    "ts-node-dev": "^2.0.0",
    "vitest": "^1.5.2"
  }
}
